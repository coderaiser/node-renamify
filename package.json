{
  "name": "renamify",
  "version": "2.0.0",
  "author": "coderaiser <mnemonic.enemy@gmail.com> (https://github.com/coderaiser)",
  "description": "rename group of files from a directory",
  "homepage": "http://github.com/coderaiser/node-renamify",
  "main": "lib/renamify.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/node-renamify.git"
  },
  "keywords": [
    "file",
    "folder",
    "rename",
    "group",
    "name"
  ],
  "scripts": {
    "test": "tape test/*.js",
    "watcher": "nodemon -w test -w lib --exec",
    "watch:test": "npm run watcher -- npm test",
    "watch:coverage": "npm run watcher -- npm run coverage",
    "lint": "redrun lint:*",
    "lint:lib": "eslint -c .eslintrc.server lib",
    "lint:server": "eslint test",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "currify": "^3.0.0",
    "itchy": "^1.0.0"
  },
  "license": "MIT",
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^5.6.1",
    "eslint-plugin-node": "^7.0.1",
    "nodemon": "^1.11.0",
    "nyc": "^13.0.1",
    "redrun": "^7.0.0",
    "tape": "^4.5.1"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
